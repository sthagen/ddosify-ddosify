{
    "iteration_count": 10,
    "load_type": "waved",
    "duration": 1,
    "steps": [
            {
              "id": 1,
              "url": "{{HTTPBIN}}/json",
              "name": "JSON",
              "method": "GET",
              "others": {
                "h2": false,
                "keep-alive": true,
                "disable-redirect": true,
                "disable-compression": false
              },
              "headers": {
                
              },
              "payload": "",
